<%- include ("partials/header")-%>

<div class="" style="margin: 0 10%">
  <h1>Home</h1>
  <p><%= startingContent %></p>

  <%allPosts.forEach(function (post) { %>

  <h1><%= post.title %></h1>
  <p>
    <%= post.content.substring(0, 100) + "..."%>
    <a href="/posts/<%= post.title %> ">Read More</a>
  </p>
  <%}) %> <%- include ("partials/footer") -%>
</div>
